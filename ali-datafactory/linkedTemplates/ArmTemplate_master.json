{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ali-datafactory"
		},
		"ingestion_dest_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ingestion_dest_datalake'"
		},
		"KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://ali-keyvaultbrief.vault.azure.net/"
		},
		"ingestion_dest_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://alidatalakebrief.dfs.core.windows.net/"
		},
		"ingestion_source_blob_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "keyblobstorage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ali-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ingestion_dest_datalake_accountKey": {
						"value": "[parameters('ingestion_dest_datalake_accountKey')]"
					},
					"KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('KeyVault_properties_typeProperties_baseUrl')]"
					},
					"ingestion_dest_datalake_properties_typeProperties_url": {
						"value": "[parameters('ingestion_dest_datalake_properties_typeProperties_url')]"
					},
					"ingestion_source_blob_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ingestion_source_blob_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}